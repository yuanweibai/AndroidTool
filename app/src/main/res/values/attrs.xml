<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundByClipImageView">
        <attr name="round_radius" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="CanvasClipView">
        <attr name="op_type" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="ShapeImageView">
        <attr name="shape" format="enum">
            <enum name="rectangle" value="1" />
            <enum name="circle" value="2" />
            <enum name="oval" value="3" />
        </attr>
        <attr name="radius" format="dimension" />
        <attr name="radius_leftTop" format="dimension" />
        <attr name="radius_leftBottom" format="dimension" />
        <attr name="radius_rightTop" format="dimension" />
        <attr name="radius_rightBottom" format="dimension" />
        <attr name="border_size" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AutoScrollCircleLayout">
        <attr name="auto_play" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InsetAttr">
        <attr name="layout_insetWay" format="enum">
            <enum name="none" value="0" />
            <enum name="padding" value="1" />
            <enum name="margin" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BladeFlashImageView">
        <attr name="flash_bitmap" format="reference" />
    </declare-styleable>

    <declare-styleable name="SpinnerDatePicker">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="line_background" format="reference|color" />
        <attr name="line_width" format="dimension" />
        <attr name="line_height" format="dimension" />
        <attr name="picker_height" format="dimension" />
        <attr name="picker_item_width" format="dimension" />
        <attr name="picker_item_interval" format="dimension" />
        <attr name="original_divider_color" format="color" />
    </declare-styleable>

    <declare-styleable name="BlurTextView">
        <attr name="blur_start_color" format="color" />
        <attr name="blur_end_color" format="color" />
        <attr name="blur_line_count" format="integer" />
    </declare-styleable>
</resources>